source "https://rubygems.org"

# Jekyll + Minimal Mistakes for Action-based builds
gem "jekyll", "~> 3.9.3"
gem "minimal-mistakes-jekyll"

# Local server compatibility for Ruby >= 3
gem "webrick"

# Plugins commonly used with Minimal Mistakes
group :jekyll_plugins do
	gem "jekyll-feed", "~> 0.6"
	gem "jekyll-seo-tag"
	gem "jekyll-sitemap"
	gem "jekyll-include-cache"
	gem "jekyll-paginate"
	gem "jekyll-gist"
end

# Markdown parser
gem "kramdown-parser-gfm"

 # If you need jekyll-admin for local admin UI uncomment below
 # gem "jekyll-admin"